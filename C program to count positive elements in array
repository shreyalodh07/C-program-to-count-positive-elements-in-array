#include <stdio.h>

int main() {
    int a[10], i, n, count = 0;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        if (a[i] > 0)
            count++;
    }

    printf("Positive elements = %d\n", count);
    return 0;
}
